<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:text="1v1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="startPressed"
        android:id="@+id/start" />

    <Button
        android:text="1vAI"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="aiPressed"
        android:id="@+id/button2" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text=""
        android:ems="10"
        android:id="@+id/txt" />

    <Button
        android:text="Change Background By URL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="changePressed"
        android:id="@+id/button" />

    <Button
        android:text="Change Background By File"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="backPressed"
        android:id="@+id/butt" />

    <Button
        android:text="Music Selection"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="musicPressed"
        android:id="@+id/music" />

    <Button
        android:text="Exit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="exitPressed"
        android:id="@+id/button3" />

    <TextView
        android:text="NoahTU: 2017"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />


</LinearLayout>